<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Calculator.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Calculator"
    Shell.FlyoutBehavior="Flyout">

    <FlyoutItem
        Title="MainMenu">
        <ShellContent ContentTemplate="{DataTemplate local:MainPage}" Title="MainPage"/>
    </FlyoutItem>

    <FlyoutItem
        Title="Calculator">
        <ShellContent ContentTemplate="{DataTemplate local:Calculator}" Title="Calculator"/>

    </FlyoutItem>

    <FlyoutItem
        Title="Integral">
        <ShellContent ContentTemplate="{DataTemplate local:IntegralPage}" Title="IntegralPage"/>

    </FlyoutItem>

</Shell>
